{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "FLASK_APP": "app.py",
    "FLASK_ENV": "production"
  },
  "buildCommand": "pip install -r requirements-vercel.txt",
  "regions": ["iad1"],
  "maxLambdaSize": "15mb"
}
